{
  "name": "open-lovable",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:worker": "tsc src/index.ts --outDir dist --target es2022 --module esnext --moduleResolution node --esModuleInterop",
    "start": "next start",
    "lint": "next lint",
    "deploy:cf": "wrangler deploy",
    "deploy:cf:dev": "wrangler dev",
    "deploy:cf:preview": "wrangler deploy --env preview",
    "deploy:cf:staging": "wrangler deploy --env staging",
    "deploy:pages": "wrangler pages deploy .next --project-name open-lovable-frontend",
    "deploy:pages:dev": "wrangler pages dev .next --project-name open-lovable-frontend",
    "test:deploy": "npm run build:worker && npm run deploy:cf:dev",
    "test:deploy:config": "node scripts/deploy-test.js",
    "pages:build": "npm run build",
    "pages:deploy": "wrangler pages deploy .next --project-name open-lovable",
    "test:integration": "node tests/e2b-integration.test.js",
    "test:api": "node tests/api-endpoints.test.js",
    "test:code": "node tests/code-execution.test.js",
    "test:gpt5": "node test/gpt5-timeout-test.js",
    "check:env": "node test/check-env.js",
    "test:api-sim": "node test/api-route-simulation.js",
    "test:all": "npm run test:integration && npm run test:api && npm run test:code"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/groq": "^2.0.0",
    "@ai-sdk/openai": "^2.0.4",
    "@anthropic-ai/sdk": "^0.57.0",
    "@e2b/code-interpreter": "^1.5.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "e2b": "^1.13.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "groq-sdk": "^0.29.0",
    "hono": "^4.9.2",
    "lucide-react": "^0.532.0",
    "next": "15.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-syntax-highlighter": "^15.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "lightningcss": "^1.25.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5",
    "wrangler": "^3.114.13"
  }
}
